<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
	<extension
         id="ru.sng.asu.katp.neural-asist.fragment"
         point="org.eclipse.e4.workbench.model">
      <fragment
            apply="always"
            uri="fragment.e4xmi">
      </fragment>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      
      <page
            class="ru.sng.asu.katp.neural-asist.preferences.OpenAIPreferencePage"
            id="ru.sng.asu.katp.neural-asist.preferences.OpenAIPreferencePage"
            name="NeuralAsist">
      </page>
        <page
            id="ru.sng.asu.katp.neural-asist.preferences.PromptsPreferencePage"
            name="Prompts"
            category="ru.sng.asu.katp.neural-asist.preferences.OpenAIPreferencePage"
            class="ru.sng.asu.katp.neural-asist.preferences.PromptsPreferencePage">
        </page>      
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="ru.sng.asu.katp.neural-asist.preferences.PreferenceInitializer">
      </initializer>
   </extension>
</plugin>
